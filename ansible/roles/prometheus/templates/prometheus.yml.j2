global:
    scrape_interval:     30s # Set the scrape interval to every 1 second.
scrape_configs:
  - job_name: 'prometheus'
    static_configs:
    - targets: ['localhost:9090', 'localhost:9100']
      labels:
        instance: prometheus
  - job_name: 'helloworld_gunicorn'
    static_configs:
      - targets: ['{{ groups['appserver'][0] }}:9102']